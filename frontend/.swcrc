{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "useBuiltins": true,
        "development": true
      }
    },
    "target": "es2020",
    "externalHelpers": true
  },
  "module": {
    "type": "es6",
    "strictMode": true
  }
}