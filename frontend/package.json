{
	"name": "frontend",
	"type": "module",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"keywords": [
		"pnpm",
		"react",
		"gitlab"
	],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.17.1",
	"scripts": {
		"build": "rsbuild build --mode production",
		"dev": "rsbuild dev --open",
		"preview": "rsbuild preview",
		"biome:format": "biome format --write src",
		"biome:lint": "biome lint --fix src",
		"biome:check": "biome check src",
		"docker:build": "docker build --force-rm --no-cache -t frontend -f ./docker/Dockerfile .",
		"docker:start": "docker run -p 80:80 --name frontend -d frontend",
		"docker:stop": "docker stop frontend",
		"docker:rm": "docker rm frontend",
		"docker:rmi": "docker rmi frontend"
	},
	"dependencies": {
		"@bem-react/classname": "^1.7.0",
		"axios": "^1.12.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-helmet-async": "^2.0.5",
		"react-router-dom": "^7.9.4",
		"zod": "^4.1.12",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@gravity-ui/browserslist-config": "^4.3.0",
		"@gravity-ui/tsconfig": "^1.0.0",
		"@biomejs/biome": "2.2.5",
		"@rsbuild/core": "^1.5.6",
		"@rsbuild/plugin-css-minimizer": "^1.0.3",
		"@rsbuild/plugin-react": "^1.4.0",
		"@types/node": "^24.7.1",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"lightningcss": "^1.30.2",
		"typescript": "^5.9.2"
	}
}
